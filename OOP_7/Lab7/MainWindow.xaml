<Window x:Class="Lab7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab7"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DataGrid Name="outgrid" Margin="0,32,0,0"/>
        <ToolBar Height="32" VerticalAlignment="Top">
            <Button Content="Submit Changes" Height="27" VerticalAlignment="Top" Click="Button_Click" HorizontalAlignment="Left" Margin="4,0"/>
            <Button Content="Load Image" Height="27" VerticalAlignment="Top" Click="Button_Click_1" HorizontalAlignment="Left" Margin="4,0"/>
            <Button Content="Test query" Height="27" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="4,0" Click="Button_Click_2"/>
            <Button Content="Show methods" Height="27" VerticalAlignment="Top" Margin="4,0" Click="Button_Click_3"/>
        </ToolBar>

    </Grid>
</Window>
